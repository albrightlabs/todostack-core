{
    "name": "albright-labs/todostack-core",
    "description": "A simple, fast to-do list application with flat-file JSON storage",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "config": {
        "optimize-autoloader": true
    }
}
